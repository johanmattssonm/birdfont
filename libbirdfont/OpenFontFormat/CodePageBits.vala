	void create_codepage (int bit, string ranges) {
		PageBit b = new PageBit (bit, ranges);
		codepages.add (b);
	}

	private class PageBit : GLib.Object {
		public GlyphRange ranges;
		public int bit;
		
		public PageBit (int bit, string unicode_ranges) {
			this.bit = bit;
			ranges = new GlyphRange ();
			
			try {
				ranges.parse_ranges (unicode_ranges);
			} catch (GLib.Error e) {
				warning (e.message);
			}
		}
		
		public bool has_char (unichar c) { 
			return ranges.has_unichar (c);
		}
		
	}
}

